{
    "compilerOptions": {
        "sourceMap": false,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strict": true,
        "strictNullChecks": true,
        "removeComments": true,
        "module": "CommonJS",
        "outDir": "./dist",
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "baseUrl": ".",
    },
    "files": [
        "src/main.ts",
    ],
    "exclude": [
        "node_modules",
    ]
}